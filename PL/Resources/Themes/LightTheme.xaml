<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryBackground">#003049</Color>
    <Color x:Key="PrimaryForeground">#000000</Color>
    <Color x:Key="ButtonBackground">#E0E0E0</Color>
    <Color x:Key="ButtonForeground">#000000</Color>
    <Color x:Key="FillRectangleBackground">#2D25C8</Color>
    <Color x:Key="FillRectangleBackgroundPurple">#FF464D90</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource PrimaryBackground}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{DynamicResource PrimaryForeground}" />
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{DynamicResource ButtonBackground}" />
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="{DynamicResource ButtonForeground}" />
    <SolidColorBrush x:Key="FillRectangle" Color="{DynamicResource FillRectangleBackground}" />
    <SolidColorBrush x:Key="FillRectangle1" Color="{DynamicResource FillRectangleBackgroundPurple}" />

</ResourceDictionary>
